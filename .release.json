{
	"pkgFiles": ["package.json"],
	"increment": "minor",
	"commitMessage": "chore: release %s",
	"tagName": "%s",
	"tagAnnotation": "Release %s",
	"buildCommand": "npm run build",
	"requireCleanWorkingDir": true,
	"src": {
		"pushRepo": "git@github.com:chriscasola/unit-ops.git",
		"beforeStageCommand": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s"
	},
	"npm": {
		"publish": true
	}
}
