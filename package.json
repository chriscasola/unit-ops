{
  "name": "unit-ops",
  "version": "0.0.0",
  "description": "Javascript library for converting and performing operations on units",
  "main": "src/index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "xo && ava"
  },
  "keywords": [
    "unit",
    "conversion",
    "quantity"
  ],
  "author": {
    "name": "Chris Casola",
    "email": "chriscasola@gmail.com",
    "url": "https://github.com/chriscasola"
  },
  "license": "MIT",
  "bugs": "https://github.com/chriscasola/unit-ops/issues",
  "homepage": "https://github.com/chriscasola/unit-ops",
  "repository": {
    "type": "git",
    "url": "https://github.com/chriscasola/unit-ops.git"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "xo": "^0.13.0"
  },
  "xo": {
    "esnext": "true",
    "ignores": [
      "lib/**"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ],
    "files": [
      "test/**/*.spec.js"
    ],
    "source": [
      "src/**/*.js"
    ]
  }
}
